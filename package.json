{
  "name": "samples-ui",
  "version": "1.0.5",
  "description": "samples-ui",
  "author": "samples",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "lib": "cross-env PKG_HOST=dev.static.s-gsun.com vue-cli-service build --target lib --name samples-ui --dest lib packages/index.js  --mode lib",
    "lib:test": "cross-env PKG_HOST=test.static.s-gsun.com vue-cli-service build --target lib --name samples-ui --dest lib packages/index.js  --mode lib",
    "lib:pre": "cross-env PKG_HOST=pre.static.s-gsun.com vue-cli-service build --target lib --name samples-ui --dest lib packages/index.js  --mode lib",
    "lib:prd": "cross-env PKG_HOST=static.s-gsun.com vue-cli-service build --target lib --name samples-ui --dest lib packages/index.js  --mode lib",
    "lib:yudao": "cross-env PKG_HOST=dev.static.s-gsun.com vue-cli-service build --target lib --name samples-ui-yd --dest lib packages/index.js  --mode lib",
    "rename": "gulp build --gulpfile ./build/gulpfile.js",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "dependencies": {
    "axios": "0.19.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-polyfill": "^6.26.0",
    "clipboard": "2.0.4",
    "codemirror": "5.45.0",
    "cross-env": "^6.0.3",
    "crypto-js": "^3.1.9-1",
    "driver.js": "0.9.5",
    "dropzone": "5.5.1",
    "echarts": "4.2.1",
    "element-china-area-data": "^4.1.2",
    "element-ui": "2.8.2",
    "file-saver": "2.0.1",
    "fuse.js": "3.4.4",
    "github-markdown-css": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-uglify": "^3.0.2",
    "highlight.js": "^9.15.10",
    "html-loader": "^0.5.5",
    "js-cookie": "2.2.0",
    "jsonlint": "1.6.3",
    "jszip": "3.2.1",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "screenfull": "4.2.0",
    "semver": "^6.3.0",
    "showdown": "1.9.1",
    "sortablejs": "1.8.4",
    "transliteration": "^2.1.7",
    "tui-editor": "1.3.3",
    "vant": "^2.4.3",
    "vue": "^2.6.10",
    "vue-count-to": "1.0.13",
    "vue-highlightjs": "^1.3.3",
    "vue-image-crop-upload": "^2.5.0",
    "vue-markdown": "^2.2.4",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "3.0.2",
    "vue-splitpane": "1.0.4",
    "vuedraggable": "2.20.0",
    "vuex": "3.1.0",
    "xlsx": "0.14.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-eslint": "3.5.1",
    "@vue/cli-plugin-unit-jest": "3.5.3",
    "@vue/cli-service": "3.5.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "23.6.0",
    "chalk": "2.4.2",
    "chokidar": "2.1.5",
    "connect": "3.6.6",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2",
    "gulp-rename": "^1.4.0",
    "highlight": "^0.2.4",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "mockjs": "1.0.1-beta3",
    "node-sass": "^4.9.0",
    "plop": "2.3.0",
    "runjs": "^4.3.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1.13.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.0",
    "vue-template-compiler": "^2.6.10"
  },
  "main": "src/main.js",
  "style": "static/index.css",
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
